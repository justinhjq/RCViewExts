<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ttyy.com.recyclerexts.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="ttyy.com.recyclerexts.demo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="ttyy.com.recyclerexts.demo.TagsVerticalActivity">
        </activity>

        <activity android:name="ttyy.com.recyclerexts.demo.TagsHorizontalActivity">
        </activity>

        <activity android:name="ttyy.com.recyclerexts.demo.DragSwapActivity">
        </activity>

        <activity android:name="ttyy.com.recyclerexts.demo.SwipeDeleteActivity">
        </activity>

    </application>

</manifest>
